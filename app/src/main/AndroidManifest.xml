<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zj.example.dagger2">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".example1.Example1Activity" />
        <activity android:name=".example2.Example2Activity" />
        <activity android:name=".example3.Example3Activity" />
        <activity android:name=".example4.Example4Activity" />
        <activity android:name=".example5.Example5Activity" />
        <activity android:name=".example6.Example6Activity" />
        <activity android:name=".example6.Example6JavaActivity" />
    </application>

</manifest>